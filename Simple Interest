import java.util.Scanner;
public class Simple
{
    public static void main(String args[])
    {Scanner sc=new Scanner(System.in);
    int a,b;
    float r;
    a=sc.nextInt();
    b=sc.nextInt();
    float c=sc.nextFloat();
    r=(a*b*c)/100;
    System.out.printf("%.2f",r);
        //write your code
    }
}
